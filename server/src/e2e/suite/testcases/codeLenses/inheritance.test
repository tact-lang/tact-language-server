trait Level1 {
    value1: Int;
    virtual fun getValue(): Int {
        return 1;
    }
}

trait Level2 with Level1 {
    value1: Int;
    override fun getValue(): Int {
        return 2;
    }
}

contract FinalContract with Level2 {
    value1: Int;
    override fun getValue(): Int {
        return 3;
    }
}
