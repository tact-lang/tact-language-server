========================================================================
Basic Function with Multiple Parameters
========================================================================
primitive Int;
primitive String;

struct User {
    first_name: String;
    last_name: String;
    age: Int;
}

contract Foo {

    fun bar(a: Int, b: String, c: User) {
        return a + 42;
    }

    init() {
        self.bar(1000, <caret>);
    }
}
------------------------------------------------------------------------
b: String
fun bar(a: Int, b: String, c: User)

